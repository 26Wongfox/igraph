
#######################################
# The basic interface
#######################################

igraph_empty:
        PARAMS: OUT GRAPH graph, INTEGER n=0, BOOLEAN directed=True
        RETURN: ERROR

igraph_add_edges:
        PARAMS: INOUT GRAPH graph, VECTOR edges, ATTRIBUTES attr
        NAME-R: add.edges
        RETURN: ERROR
        IGNORE: RR, RC

igraph_vcount:
        PARAMS: GRAPH graph
        NAME-R: vcount
        RETURN: INTEGERPTR

#######################################
# Components
#######################################

igraph_clusters:
        PARAMS: GRAPH graph, OUT VECTOR membership, OUT VECTOR csize, \
	        OUT INTEGERPTR no, CONNECTEDNESS mode=WEAK
        NAME-R: clusters
        RETURN: ERROR

#######################################
# Layouts
#######################################

igraph_layout_fruchterman_reingold:
        PARAMS: GRAPH graph, OUT MATRIX res, INTEGER niter, REAL maxdelta, \
                REAL area, REAL coolexp, REAL repulserad, NULL use_seed, \
                VECTOR_OR_0 weight
        NAME-R: layout.fruchterman.reingold
        RETURN: ERROR
        FLAGS: PROGRESS
        IGNORE: RR

igraph_layout_graphopt:
        PARAMS: GRAPH graph, OUT MATRIX res, INTEGER niter=500, \
                REAL node_charge=0.001, REAL node_mass=30, \
                INTEGER spring_length=0, REAL spring_constant=1, \
                REAL max_sa_movement=5, NULL use_seed
        NAME-R: layout.graphopt
        RETURN: ERROR
        FLAGS: PROGRESS

#######################################
# Structure generators
#######################################

igraph_lcf_vector:
        PARAMS: OUT GRAPH graph, INTEGER n, VECTOR shifts, INTEGER repeats
        NAME-R: graph.lcf
        RETURN: ERROR

igraph_dyad_census:
        PARAMS: GRAPH graph, OUT INTEGERPTR mut, OUT INTEGERPTR asym, OUT INTEGERPTR null
        NAME-R: dyad.census
        RETURN: ERROR

igraph_triad_census:
        PARAMS: GRAPH graph, OUT VECTOR res
        NAME-R: triad.census
        RETURN: ERROR
