
FUNCTION igraph_empty:			# name of the function in the C source
	ARG:	OUT GRAPH graph		# argument 1, output a GRAPH here
	ARG:	INTEGER n		# argument 2, input an INTEGER
	ARG:	BOOL directed		# argument 3, input a BOOL
	RETURN:	graph			# return with the graph argument
	DEFAULT:n "0"			# n=0 is the default value
	DEFAULT:directed "TRUE"		# directed=TRUE is the default value
	INAME:	R-R graph.empty		# in R function renamed to graph.empty
	INAME:	R-NAMESPACE graph.empty

