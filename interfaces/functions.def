
FUNCTION igraph_empty:			# name of the function in the C source
	ARG:	OUT GRAPH graph		# argument 1, output a GRAPH here
	ARG:	INTEGER n		# argument 2, input an INTEGER
	ARG:	BOOL directed		# argument 3, input a BOOL
	RETURN:	graph			# return with the graph argument
	DEFAULT:n "0"			# n=0 is the default value
	DEFAULT:directed "TRUE"		# directed=TRUE is the default value
	INAME:	R-R graph.empty		# in R function renamed to graph.empty
	INAME:	R-NAMESPACE graph.empty
	FLAG:	SHOUT graph
	IGNORE:	R-R R-C R-NAMESPACE

FUNCTION igraph_add_edges:
	ARG:	INOUT GRAPH graph
	ARG:	IN VECTOR edges
	ARG:	IN ATTRIBUTES attr
	RETURN:	graph
	INAME:	R-R add.edges
	INAME:	R_NAMESPACE add.edges
	IGNORE:	R-R R-C R-NAMESPACE
	FLAG:	SHIN graph
	FLAG:	SHOUT graph

FUNCTION igraph_vcount:
	ARG:	IN GRAPH graph
	RETTYPE: INTEGER	        # return type is not int
	RETURN:	RETURN                  # the output of the func. is returned
	INAME:	R-R vcount
	INAME:	R-NAMESPACE vcount
	IGNORE:	R-R R-C R-NAMESPACE
	FLAG:	SHIN graph
	FLAG:	SHOUT RETURN
