        NAME-R: neighbors
        IGNORE: RR, RC, RNamespace

igraph_is_directed:
        PARAMS: GRAPH graph
        NAME-R: is.directed
        RETURN: BOOLEAN
        IGNORE: RR, RC, RNamespace

igraph_degree:
